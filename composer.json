{
    "name": "appkeep/laravel-appkeep",
    "description": "Instantly discover problems with your Laravel sites.",
    "keywords": [
        "appkeep",
        "laravel",
        "monitoring",
        "health-checks"
    ],
    "homepage": "https://github.com/appkeep/laravel-appkeep",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Appkeep",
            "email": "hello@appkeep.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "orchestra/testbench": "5.*|6.*|7.*",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Appkeep\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appkeep\\Laravel\\AppkeepProvider"
            ],
            "aliases": {
                "Appkeep": "Appkeep\\Laravel\\Facades\\Appkeep"
            }
        }
    }
}